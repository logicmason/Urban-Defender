import mochi.as2.*;import ui.EventLog;class Init{    public static var GAME_ID:String  = "84993a1de4031cd8";    public static var AD_ID:String    = "test";  // This is normally your GAME_ID    public static var clip:MovieClip;    public static var overlay:MovieClip;        public static function main( container:MovieClip ):Void    {        clip = container.createEmptyMovieClip( '_coreClip', container.getNextHighestDepth() );        overlay = container.createEmptyMovieClip( '_overlayClip', container.getNextHighestDepth() );                var init:Init = new Init();    }    public function Init()    {        Stage.scaleMode = 'noScale';        Stage.align = 'TL';                MochiAd.showPreGameAd( {            id: AD_ID,            // This is the game ID for displaying ads!            clip: clip,             // We are displaying in a container (which is dynamic)            ad_finished: init      // Ad has completed        } );    }        private function init():Void    {        MochiServices.connect( GAME_ID, _root, onFailure );        Core.init();    }    private function onFailure(e):Void    {        Core.popup('MochiServices failed to connect.');    }}